%header#header
  .navbar.navbar-fixed-top
    .navbar-inner
      .container
        %a.brand{:href => "#"}
          = image_tag "logo2.png"
        %ul.nav.pull-right
          %li.active
            %a{:href => "#"} Home
          - if !user_signed_in?
            %li= link_to "Sign up", new_user_registration_path
            %li= link_to "Login", new_user_session_path
          - else
            %li= link_to "Logout", destroy_user_session_path, :method => :delete
